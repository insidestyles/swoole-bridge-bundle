{
    "name": "insidestyles/swoole-bridge-bundle",
    "description": "Swoole Bridge For Php Frameworks",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "fuong",
            "email": "insidestyles@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^5.0|^6.0",
        "insidestyles/swoole-bridge": "^1.0",
        "symfony/psr-http-message-bridge": "^1.0|^2.0",
        "symfony/console": "^5.0|^6.0",
        "laminas/laminas-diactoros": "^2.0",
        "nyholm/psr7": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5",
        "eaglewu/swoole-ide-helper": "dev-master"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Insidestyles\\SwooleBridgeBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "Tests/DependencyInjection"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
